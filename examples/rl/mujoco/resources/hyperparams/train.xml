<HyperParameters>
  <Examples>

    <!--Devices-->
    <Agent>
      <StateValue>
        <device value="GPU" type="string"/>
      </StateValue>
      <StochasticPolicy>
        <device value="GPU" type="string"/>
      </StochasticPolicy>
    </Agent>

    <!-- Seeds -->
    <graph_seed value="0" type="int"/>
